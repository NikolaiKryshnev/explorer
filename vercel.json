{
  "rewrites": [
    {
      "source": "/blocks/:path*",
      "destination": "/blocks/:path*.tsx"
    },
    {
      "source": "/block/:path*",
      "destination": "/block/:path*.tsx"
    },
    {
      "source": "/txn/:path*",
      "destination": "/txn/:path*.tsx"
    },
    {
      "source": "/account/:address?network=:network",
      "destination:": "/account/:address.tsx?network=:network"
    }
  ]
}
